<Application x:Class="SpellCheck.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:SpellCheck"
             xmlns:vm="clr-namespace:SpellCheck.View"
             StartupUri="View/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/SpellCheck;component/Resources/WindowStyle.xaml" />
                <ResourceDictionary Source="/SpellCheck;component/Resources/DataGridStyle.xaml" />
                <ResourceDictionary Source="/SpellCheck;component/Resources/Misc.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>




    </Application.Resources>
</Application>
